# n8n Basic Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-secure-password-here

# Database Configuration (Railway/Render will auto-populate these)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=${PGHOST}
DB_POSTGRESDB_PORT=${PGPORT}
DB_POSTGRESDB_DATABASE=${PGDATABASE}
DB_POSTGRESDB_USER=${PGUSER}
DB_POSTGRESDB_PASSWORD=${PGPASSWORD}

# Server Configuration
WEBHOOK_URL=https://your-app-name.up.railway.app/
N8N_HOST=0.0.0.0
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_EDITOR_BASE_URL=https://your-app-name.up.railway.app/

# API Keys
OPENAI_API_KEY=your-openai-api-key-here

# n8n Features
N8N_METRICS=false
EXECUTIONS_PROCESS=main
N8N_LOG_LEVEL=info

# Security
N8N_SECURE_COOKIE=true
N8N_JWT_AUTH_ACTIVE=true
N8N_JWT_AUTH_HEADER=authorization